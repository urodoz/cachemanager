{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
{% set icon %}
<img height="28" alt="Urodoz Cache Manager" src="data:image/png;base64,">
<span class="sf-toolbar-info-piece-additional-detail">Cache</span>
{% endset %}
{% set text %}
<div class="sf-toolbar-info-piece">
    <b>Urodoz Cache</b>
</div>
<div class="sf-toolbar-info-piece">
    <b>Hits</b>
    <span class="sf-toolbar-status sf-toolbar-status-yellow">{{ collector.hits|length }}</span>
</div>
<div class="sf-toolbar-info-piece">
    <b>Miised hits</b>
    <span class="sf-toolbar-status sf-toolbar-status-red">{{ collector.missedHits|length }}</span>
</div>
{% endset %}
{% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}

{% endblock %}

{% block head %}
    {# if the web profiler panel needs some specific JS or CSS files #}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon">
        <img width=30 src="{{ asset('bundles/urodozcache/images/cache.png') }}" />
    </span>
    <strong>Cache manager</strong>
    <span class="count">
        <span>{{ collector.hits|length }}</span>
    </span>
</span>
{% endblock %}

{% block panel %}
<h2>Urodoz Cache Manager</h2>
{% endblock %}
